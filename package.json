{
  "name": "previous-date-range",
  "version": "0.3.0",
  "description": "Calculate a date range in the past from a certain moment",
  "main": "lib/previous-date-range.js",
  "scripts": {
    "prepublish": "npm run build",
    "build": "node_modules/.bin/coffee -o lib -c src/previous-date-range.coffee",
    "lint": "node_modules/.bin/eslint ./src",
    "watch": "node_modules/.bin/coffee -o lib -w -c src/previous-date-range.coffee",
    "test": "node_modules/.bin/mocha -R spec"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jamiter/previous-date-range.git"
  },
  "keywords": [
    "moment",
    "date",
    "previous"
  ],
  "author": "Michiel ter Reehorst",
  "license": "UNLICENSE",
  "bugs": {
    "url": "https://github.com/jamiter/previous-date-range/issues"
  },
  "homepage": "https://github.com/jamiter/previous-date-range",
  "dependencies": {
    "moment": "^2.6.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "coffee-script": "^1.12.2",
    "eslint": "^3.13.1",
    "eslint-config-airbnb-base": "^11.0.1",
    "eslint-plugin-import": "^2.2.0",
    "mocha": "^3.2.0"
  }
}
