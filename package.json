{
  "name": "previous-date-range",
  "version": "0.4.0",
  "description": "Calculate a date range in the past from a certain moment",
  "main": "lib/previous-date-range.js",
  "scripts": {
    "prepublish": "npm run build -s && npm test -s",
    "build": "babel src -d lib",
    "lint": "eslint ./src ./test",
    "mocha": "mocha -R spec",
    "start": "npm run watch",
    "test": "npm run lint -s && npm run mocha",
    "watch": "npm run build -s -- --watch"
  },
  "repository": {
    "type": "git",
